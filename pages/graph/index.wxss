.container{
    width: 100%;
    height: 100%;
    padding: 10rpx;
    box-sizing: border-box;
    overflow-x:hidden;
    background: #f5f5f5;
}
.wrapper{
    width: 100%;
    /* height: 100%; */
    padding: 0 10rpx;
    box-sizing: border-box;
    background: #fff;
    border-radius: 10rpx;
}
.wrapper .item{
    width: 100%;
    height: 80rpx;
    border-bottom: 1rpx solid #f5f5f5;
}
.item text{
    display: block;
    height: 100%;
    line-height: 80rpx;
    font-size: 24rpx;
    color: #333;
}
.item image{
    display: block;
    width: 40rpx;
    height: 40rpx;
}